.contact-header-title
  font-size 28px
.contact-header-title-box
  padding-top 84px
.contact-body-container
  padding-top 22px
.contact-input-box
  width 330px
  height 35px
  margin-bottom 12px
.contact-input-box-textarea
  width 330px
  height 100px
  margin-bottom 14px
.contact-input 
  width 330px
  border solid 1px #e5e5e5
  background-color #FFFFFF
  color #9b9b9b
  font-size 11.5px
  padding-left 20px
  border-radius 3px
  height 35px
.contact-input::placeholder
  color #9b9b9b
  font-size 11.5px
textarea.contact-input
  height 100px
  resize none
  padding-top 10px
.contact-way-text
  font-size 11.5px
  line-height 26px
  padding-left 12px
.contact-checkbox-box
  justify-content flex-start !important
  width 330px
.contact-btn
  background-color #6b806c
  border none
  width 94px
  height 29px
  font-size 11px
  margin-top 6px
  border-radius 20px
  text-align center
  margin auto
  transition all 0.1s linear
  -webkit-transition all 0.1s linear
  -o-transition all 0.1s linear
  -moz-transition all 0.1s linear
.contact-last-button-container
  padding-top 20px
  padding-bottom 40px
.contac-map-container
  width 100%
  height 430px
  position relative
.contact-map
  width 100%
  height 430px
.map-info-title
  font-size 28px
  padding-top 20px
.map-info-address
  font-size 13px
  padding-top 20px
  padding-bottom 15px
  width 214px
  text-align center
.fixed-map-info-box
  width 310px
  height 190px
  background-color #6d989c
  position absolute
  left 49px
  top -54px
  z-index +1
  justify-content flex-start !important
  